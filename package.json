{
  "name": "miraski-website",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm version minor -m \"chore: Release version %s\" && npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "firebase": "^10.9.0",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "react-firebase-hooks": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "./util/(.*)$",
      "./pages/(.*)$",
      "./components/(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
